doctype
html
  head
    title Dashboard
    link(rel='stylesheet', href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css")
    link(rel='stylesheet', href="/css/demo.css")
    script(src="https://code.jquery.com/jquery-2.1.1.min.js")
    script(src="/js/demo.js")
    script(src="/js/ajaxq.js")
  body
    .container
      h1 Home Automation Dashboard
      .row.voffset30
        .col-md-2.text30
          div WiFi
        .col-md-2
          button.btn.btn-block.btn-lg.btn-primary#1 On
        .col-md-2
          button.btn.btn-block.btn-lg.btn-danger#2 Off
        .col-md-2.text20 
          div#wifiTemperature Temperature:
        .col-md-2.text20 
          div#wifiStatus Status
      .row.voffset30
        .col-md-2.text30
          div Bluetooth
        .col-md-2
          button.btn.btn-block.btn-lg.btn-primary#3 On
        .col-md-2
          button.btn.btn-block.btn-lg.btn-danger#4 Off
        .col-md-2.text20
          div#bluetoothTemperature Temperature:
        .col-md-2.text20 
          div#bluetoothStatus Status
      .row.voffset30
        .col-md-2.text30
          div XBee
        .col-md-2
          button.btn.btn-block.btn-lg.btn-primary#5 On
        .col-md-2
          button.btn.btn-block.btn-lg.btn-danger#6 Off
        .col-md-2.text20
          div#xbeeTemperature Temperature:
        .col-md-2.text20 
          div#xbeeStatus Status